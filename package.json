{
  "name": "forkify",
  "version": "1.0.0",
  "description": "Recipe application",
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html --dist-dir ./dist"
  },
  "author": "Enyinnaya wisdom",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.9.3",
    "parcel": "^2.9.3",
    "process": "^0.11.10",
    "sass": "^1.26.10"
  },
  "dependencies": {
    "core-js": "^3.31.1",
    "fractional": "^1.0.0",
    "regenerator-runtime": "^0.13.11",
    "stable": "^0.1.8"
  },
  "browserslist": [
    "defaults"
  ],
  "bundlerConfig": {
    "postcss": {
      "plugins": {
        "autoprefixer": {
          "grid": true
        },
        "postcss-svg": {
          "cleanupIDs": false
        }
      }
    }
  },
  "plugins": [
    {
      "name": "preset-default"
    },
    "cleanupIDs"
  ]
}
